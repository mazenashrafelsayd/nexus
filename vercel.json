{
  "version": 2,
  "builds": [
    {
      "src": "app/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/users/(.*)",
      "dest": "app/index.js"
    },
    {
      "src": "/about",
      "dest": "frontend/dist/index.html"
    },
    {
      "src": "/jobs",
      "dest": "frontend/dist/index.html"
    },
    {
      "src": "/blog",
      "dest": "frontend/dist/index.html"
    },
    {
      "src": "/products/blockchain",
      "dest": "frontend/dist/index.html"
    },
    {
      "src": "/products/ai",
      "dest": "frontend/dist/index.html"
    },
    {
      "src": "/products/consulting",
      "dest": "frontend/dist/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/dist/$1"
    }
  ]
}